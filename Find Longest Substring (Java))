class Solution{
public int longsubstr(string s){
  int n=s.length();                                            //intialise
  int maxl=0;
  int left=0;
  Set <Character> charSet=new HashSet();
  for(int right=0;right<n;right++){
    if(!charSet.contains(s.charAt(right))){
      charSet.append(s.charAt(right));
      maxl= Math.max(maxl, right-left+1);
    else{
      if(charSet.contains(s.charAt(right))){
       chatrSet.remove(s.charAt(right));
      left++;
      }
    }
  charSet.append(s.charAt(s));
    }
  }
return maxl;
}
